    "# Apply get_top function to create subset of top 1000 names\n",
    "n = 1000\n",
    "grouped = names.groupby(['year', 'sex'])\n",
    "top1000 = grouped.apply(get_top, n=n)\n",
    "top1000.reset_index(inplace=True, drop=True)\n",
    "top1000.head(10)"
   ]

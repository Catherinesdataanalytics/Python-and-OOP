   "source": [
    "# Write function to report the most common time zones\n",
    "def top_counts(count_dict, n=10):#default n=10\n",
    "    value_key_pairs = [(count, tz) for tz, count in count_dict.items()]#value,key,count\n",
    "    value_key_pairs.sort()\n",
    "    return value_key_pairs[-n:]"
   ]
  },

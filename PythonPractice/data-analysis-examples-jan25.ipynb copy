    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Define function to count the number of records that contain each unique time zone\n",
    "def get_counts(sequence):\n",
    "    counts = {}\n",
    "    for x in sequence:\n",
    "        if x in counts:\n",
    "            counts[x] += 1\n",
    "        else:\n",
    "            counts[x] = 1\n",
    "    return counts"
   ]
  },
